import px
import pxtrace

# The UPID of the simple-gotracing-example server.
upid = '00000003-0024-844a-0000-000008caa618'

@pxtrace.goprobe("main.computeE")
def probe_func():
    return [{'iterations': pxtrace.ArgExpr("iterations")}]


pxtrace.UpsertTracepoint('compute_e_data',
                         'compute_e_data',
                         probe_func,
                         px.uint128(upid),
                         "5m")

px.display(px.DataFrame(table='compute_e_data'))
